# VoxSurf Environment Variables
# Copy this file to .env and fill in your values

# OpenAI API Key (optional - can also be set in extension popup)
# This is for development/testing purposes
OPENAI_API_KEY=sk-your-api-key-here

# Development mode settings
NODE_ENV=development

# Vite configuration
VITE_APP_NAME=VoxSurf
VITE_APP_VERSION=1.0.0

# MediaPipe CDN (optional - defaults to jsdelivr)
# VITE_MEDIAPIPE_CDN=https://cdn.jsdelivr.net/npm/@mediapipe/tasks-vision@0.10.0

# Readability CDN (optional - defaults to jsdelivr)
# VITE_READABILITY_CDN=https://cdn.jsdelivr.net/npm/@mozilla/readability@0.4.4/Readability.js

# Debug mode (enables console logging)
VITE_DEBUG=false
